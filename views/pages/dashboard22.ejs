  <script>
    $('#open').click(function() {
      $('#modalOverlay').show().addClass('modal-open');
    });



    // Messages toggle
    $("#dashboard").on('click', '#YourMessages , #NewMessages, #NewMessages2', () => {
      $("#dashboard").closest(".dashToggle").addClass("hidden");
      $("#projects").removeClass('hidden')
    })

    $("#navDash").on('click', () => {

      $("#projects").hide();
      $('#invoices').hide();
      $("#dashboard").show();
    })

    $("#navInvoc").on('click' , ()=>
    {
      $('#invoices').show();
      $("#dashboard").hide();
      $("#projects").hide();
    })

    $("#navProj").on('click', () => {
      $("#dashboard").hide();
      $("#projects").show();
      $('#invoices').hide();
    })
  </script>
